## Part Two

### Code - Part 2

```elixir
defmodule PartTwo do
  require Integer, [:is_odd, :is_even]

  def solve(input) do
    IO.puts("--- Part Two ---")
    IO.puts("Result: #{run(input)}")
  end

  def run(inputs), do: run(inputs, 1_000_000_000)

  def run(input_string, cycles) when is_bitstring(input_string),
    do: run(Parser.parse(input_string), cycles)

  def run(inputs, cycles) do
    inputs
    |> Enum.map(fn input ->
      Enum.reduce(1..cycles, input, fn cycle_iteration, output ->
        if rem(cycle_iteration, 10_000) == 0, do: IO.inspect(cycle_iteration)
        spin_cycle(output)
      end)
      |> PartOne.total_load()
    end)
    |> Enum.sum()
  end

  def spin_cycle(input) do
    # |> tilt_south() |> tilt_east()
    input |> PartOne.tilt_north() |> tilt_west() |> tilt_south() |> tilt_east()
  end

  defp tilt_west(input) do
    # IO.inspect(0..(length(input) - 1), label: :row_range)
    # IO.inspect(0..((input |> hd() |> length()) - 1), label: :col_range)
    Enum.reduce(0..(length(input) - 1), input, fn row_i, output ->
      Enum.reduce(0..((output |> hd() |> length()) - 1), output, fn col_i, output ->
        # IO.inspect({row_i, col_i}, label: :coord)
        roll_piece_west(output, row_i, col_i)
      end)
    end)
  end

  defp roll_piece_west(input, row_idx, col_idx) do
    if PartOne.piece_at(input, row_idx, col_idx) != "O" do
      input
    else
      start_point = {row_idx, col_idx}

      end_point = {
        row_idx,
        Enum.find(col_idx..0, fn i ->
          # IO.inspect({i, {i - 1, col_idx}, piece_at(input, i - 1, col_idx)}, label: :finding_coord_to_move_to)
          i == 0 || PartOne.piece_at(input, row_idx, i - 1) != "."
        end)
      }

      # IO.inspect({input, start_point, end_point}, label: :input_to_move_piece)
      PartOne.move_piece(input, start_point, end_point)
    end
  end

  defp tilt_south(input) do
    # IO.inspect(0..(length(input) - 1), label: :row_range)
    # IO.inspect(0..((input |> hd() |> length()) - 1), label: :col_range)
    Enum.reduce((length(input) - 1)..0, input, fn row_i, output ->
      Enum.reduce(0..((output |> hd() |> length()) - 1), output, fn col_i, output ->
        # IO.inspect({row_i, col_i}, label: :coord)
        roll_piece_south(output, row_i, col_i)
      end)
    end)
  end

  defp roll_piece_south(input, row_idx, col_idx) do
    if PartOne.piece_at(input, row_idx, col_idx) != "O" do
      input
    else
      start_point = {row_idx, col_idx}

      end_point = {
        Enum.find(row_idx..(length(input) - 1), fn i ->
          # IO.inspect({i, {i - 1, col_idx}, piece_at(input, i - 1, col_idx)}, label: :finding_coord_to_move_to)
          i == length(input) - 1 || PartOne.piece_at(input, i + 1, col_idx) != "."
        end),
        col_idx
      }

      # IO.inspect({input, start_point, end_point}, label: :input_to_move_piece)
      PartOne.move_piece(input, start_point, end_point)
    end
  end

  defp tilt_east(input) do
    # IO.inspect(0..(length(input) - 1), label: :row_range)
    # IO.inspect(0..((input |> hd() |> length()) - 1), label: :col_range)
    Enum.reduce(0..(length(input) - 1), input, fn row_i, output ->
      Enum.reduce(((output |> hd() |> length()) - 1)..0, output, fn col_i, output ->
        # IO.inspect({row_i, col_i}, label: :coord)
        roll_piece_east(output, row_i, col_i)
      end)
    end)
  end

  defp roll_piece_east(input, row_idx, col_idx) do
    if PartOne.piece_at(input, row_idx, col_idx) != "O" do
      input
    else
      start_point = {row_idx, col_idx}

      end_point = {
        row_idx,
        Enum.find(col_idx..(length(hd(input)) - 1), fn i ->
          # IO.inspect({i, {i - 1, col_idx}, piece_at(input, i - 1, col_idx)}, label: :finding_coord_to_move_to)
          i == length(hd(input)) || PartOne.piece_at(input, row_idx, i + 1) != "."
        end)
      }

      # IO.inspect({input, start_point, end_point}, label: :input_to_move_piece)
      PartOne.move_piece(input, start_point, end_point)
    end
  end
end
